{
    "manifest_version": 2,
  
    "name": "atiSearch",
    "description": "This extension atiSearch.",
    "version": "1.0",
  
    "permissions": [
      "https://www.google.com/",
      "notifications", "contextMenus"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup00": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
      ,
      "background": {
        "scripts": ["background.js"]
      }
  }